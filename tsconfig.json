{
  "compilerOptions": {
    "target": "ESNext",                             
    "module": "CommonJS",
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "rootDir": "./src/",
    "outDir": "./src/main/resources/static/javascript",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["build"]
}
